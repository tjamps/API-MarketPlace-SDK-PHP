{
    "name": "cdiscount/sdkphpapi",
    "description": "PHP SDK for the Cdiscount Marketplace API",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Sdk\\": "sdk/src/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-db": "^2.8",
        "zendframework/zend-soap": "^2.6",
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-config": "^2.6"
    },
    "license": "GPL-3.0",
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "rector/rector": "^0.7.22"
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix --allow-risky yes --config .php_cs.dist -vvv",
        "phpcompatinfo": "phpcompatinfo analyser:run sdk"
    }
}
